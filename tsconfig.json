{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": [
      "es2015"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
    },
    "baseUrl": ".",
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "./*"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
}